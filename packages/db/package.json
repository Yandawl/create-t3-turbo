{
  "name": "@webmeric/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/ && rimraf node_modules/.cache/tsbuildinfo.json && tsc --noEmit false --outDir dist",
    "dev": "tsc",
    "clean": "rm -rf .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.10.1",
    "mongoose": "^8.5.2"
  },
  "devDependencies": {
    "@webmeric/eslint-config": "workspace:*",
    "@webmeric/prettier-config": "workspace:*",
    "@webmeric/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@webmeric/prettier-config"
}
